.quick-item {
  width: 19.5%;
  display: inline-block;
  box-sizing: border-box;
  margin-top: 10px;
}

.quick-image {
  width: 43px;
  height: 43px;
  margin: 0 auto;
  border-radius: 50%;
  overflow: hidden;
}

.quick-image image {
  width: 100%;
  height: 100%;
}

.quick-name {
  text-align: center;
  font-size: 12px;
  line-height: 1.5em;
  margin-top: 7px;
  font-weight: 500;
  color: #666;
}